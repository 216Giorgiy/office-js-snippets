{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true,
        "removeComments": false,
        "noImplicitAny": false,
        "noEmitOnError": true,
        "allowUnusedLabels": false,
        "noUnusedParameters": false,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "rootDir": "config",
        "lib": [
            "dom",
            "es2015"
        ],
        "pretty": true
    }
}